{
    "name": "gemz/port",
    "description": "Domain Port Check",
    "keywords": [
        "gemz",
        "port",
        "reactPhp"
    ],
    "homepage": "https://github.com/gemzio/port",
    "license": "MIT",
    "authors": [
        {
            "name": "Stefan Riehl",
            "email": "stefan@sriehl.com",
            "homepage": "https://gemz.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "react/promise": "^2.7"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.3",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^8.2",
        "phpstan/phpstan": "^0.12.8"
    },
    "autoload": {
        "psr-4": {
            "Gemz\\Port\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gemz\\Port\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    }
}
